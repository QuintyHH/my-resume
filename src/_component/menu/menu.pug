-
  var menuList= [
    {
      label: '#home',
      icon: 'home-outline',
      url: '#landing-page',
    },
      {
      label: '#about-me',
      icon: 'user-outline',
      url: '#about-me',
    },
      {
      label: '#my-diplomas',
      icon: 'pen',
      url: '#award',
    },
      {
      label: '#my-skills',
      icon: 'spanner-outline',
      url: '#skills',
    },
      {
      label: '#my-projects',
      icon: 'lightbulb',
      url: '#projects',
    },
  ]

#menu
a.menu__to-go(href="#menu")
  span.my-cv__icon--th-menu-outline
.menu
  .menu__list
    each menu in menuList  
      a.menu__url(href=menu.url)
        span(className=`my-cv__icon--${menu.icon}`)
        span.menu__label=menu.label